
{
  "parser": "babel-eslint",
  "extends": [
    "eslint-config-airbnb"
  ],
  "rules": {
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "no-shadow": "off",
    "no-underscore-dangle": "off",
    "react/forbid-prop-types": ["error", { "forbid": ["any"] }],
    "react/jsx-filename-extension": "off"
  }
}